lfit<- lm(RoomRent~ Date + HotelName+ IsWeekend + IsNewYearEve+ StarRating + Airport + FreeWifi+FreeBreakfast +HotelCapacity+HasSwimmingPool,data=indore.df)
summary(lfit)

summary(indore.df)
has <- indore.df[ which(indore.df$FreeWifi=='1' & indore.df$FreeBreakfast=='1' & indore.df$HasSwimmingPool=="1") , ] 
View(has)
median(has$RoomRent)

doesnthave<- indore.df[ which(indore.df$FreeBreakfast=='0') , ] 
median(doesnthave$RoomRent)

lfit<- lm(RoomRent~ IsWeekend + IsNewYearEve,data=indore.df)
summary(lfit)
summary(lfit)
