# NAME : Vidya Preetha Anandamurali
#Analysis of Individual Cities(The analysis for the combined csv file along with regression is given below the set of 42 individual cities' analysis)
#mumbai
mumbai.df <- read.csv(paste("0 Mumbai.csv", sep="")) #to read the file
View(mumbai.df)
summary(mumbai.df)
table(mumbai.df$HotelName)
library(lattice)
barchart(HotelName~RoomRent,data = mumbai.df)

barchart(HotelName~ StarRating,data=mumbai.df)
dim(mumbai.df)
library(lattice)
bwplot(HotelName ~Airport,data=mumbai.df,horizontal = TRUE)

table(mumbai.df$HotelName, mumbai.df$FreeWifi&mumbai.df$FreeBreakfast)
table(mumbai.df$HotelName , mumbai.df$IsNewYearEve|mumbai.df$IsWeekend)
par(mfrow=c(3,1))
with(mumbai.df,plot(mumbai.df$Airport , mumbai.df$RoomRent))
with(mumbai.df,plot(mumbai.df$FreeWifi&mumbai.df$FreeBreakfast , mumbai.df$RoomRent))
with(mumbai.df,plot(mumbai.df$IsNewYearEve|mumbai.df$IsWeekend,mumbai.df$RoomRent))
mumbai.df[1:5]<- list(NULL)
mumbai.df[3]<- NULL
mumbai.df[7:9] <- list(NULL)
mumbai.df[9:10]<- list(NULL)
library(corrgram)
corrgram(mumbai.df,order = TRUE, lower.panel = panel.shade,upper.panel = panel.pie,text.panel=panel.txt)
cor(mumbai.df[,c(1,2,4,5,6,7,8)])



myfit<- lm(RoomRent~ StarRating+ Airport,data=mumbai.df)
summary(myfit)

lfit<- lm(RoomRent~ IsWeekend + IsNewYearEve,data=mumbai.df)
summary(lfit)

lfit<- lm(RoomRent~ Date + HotelName + StarRating + Airport + FreeWifi+FreeBreakfast +HotelCapacity+HasSwimmingPool,data=mumbai.df)
summary(lfit)

#Delhi
delhi.df <- read.csv(paste("1 Delhi.csv", sep="")) #to read the file
View(delhi.df)
summary(delhi.df)
barchart(HotelName~RoomRent,data = delhi.df)
barchart(HotelName~ StarRating,data=delhi.df)
library(lattice)
bwplot(HotelName ~Airport,data=delhi.df,horizontal = TRUE)

table(delhi.df$HotelName, delhi.df$FreeWifi&delhi.df$FreeBreakfast)
table(delhi.df$HotelName , delhi.df$IsNewYearEve|delhi.df$IsWeekend)
par(mfrow=c(3,1))
with(delhi.df,plot(delhi.df$Airport , delhi.df$RoomRent))
with(delhi.df,plot(delhi.df$FreeWifi&delhi.df$FreeBreakfast , delhi.df$RoomRent))
with(delhi.df,plot(delhi.df$IsNewYearEve|delhi.df$IsWeekend,delhi.df$RoomRent))
delhi.df[1:5]<- list(NULL)
delhi.df[3]<- NULL
delhi.df[7:9] <- list(NULL)
delhi.df[9:10]<- list(NULL)
library(corrgram)
corrgram(delhi.df,order = TRUE, lower.panel = panel.shade,upper.panel = panel.pie,text.panel=panel.txt)
cor(delhi.df[,c(1,2,4,5,6,7,8)])



table(delhi.df$HotelName)

myfit<- lm(RoomRent~ StarRating+ Airport,data=delhi.df)
summary(myfit)

lfit<- lm(RoomRent~ IsWeekend + IsNewYearEve,data=delhi.df)
summary(lfit)

lfit<- lm(RoomRent~ Date + HotelName + StarRating + Airport + FreeWifi+FreeBreakfast +HotelCapacity+HasSwimmingPool,data=delhi.df)
summary(lfit)

