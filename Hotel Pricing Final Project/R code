with(indore.df,plot(indore.df$Airport , indore.df$RoomRent))
with(indore.df,plot(indore.df$FreeWifi&indore.df$FreeBreakfast , indore.df$RoomRent))
with(indore.df,plot(indore.df$IsNewYearEve|indore.df$IsWeekend,indore.df$RoomRent))
indore.df[1:5]<- list(NULL)
indore.df[3]<- NULL
