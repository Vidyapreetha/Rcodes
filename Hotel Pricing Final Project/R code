#jodhpur
jodhpur.df <- read.csv(paste("22 Jodhpur.csv", sep="")) #to read the file
View(jodhpur.df)
summary(jodhpur.df)
dim(jodhpur.df)
barchart(HotelName~ StarRating,data=jodhpur.df)
library(lattice)
bwplot(HotelName ~Airport,data=jodhpur.df,horizontal = TRUE)

table(jodhpur.df$HotelName, jodhpur.df$FreeWifi&jodhpur.df$FreeBreakfast)
table(jodhpur.df$HotelName , jodhpur.df$IsNewYearEve|jodhpur.df$IsWeekend)
