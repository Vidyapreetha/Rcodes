#Combined Analsis of all 42 cities
#Reading the data
cities.df<- read.csv(paste("Cities42.csv", sep=""))
attach(cities.df)
View(cities.df)

library(psych)
describe(cities.df)[,c(2,3,4,5,8,9)]

#Visualisation
par(mfrow=c(4,1))
with(cities.df,plot(cities.df$Airport , cities.df$RoomRent))
with(cities.df,plot(cities.df$FreeWifi , cities.df$RoomRent))
with(cities.df,plot(cities.df$IsNewYearEve|cities.df$IsWeekend,cities.df$RoomRent))
with(cities.df,plot(cities.df$IsTouristDestination,cities.df$RoomRent))

table(cities.df$FreeWifi&cities.df$FreeBreakfast)
table(cities.df$IsNewYearEve|cities.df$IsWeekend)

aggregate(cbind(FreeWifi,FreeBreakfast,HasSwimmingPool,RoomRent)~ CityName, data=cities.df,mean)

boxplot(RoomRent ~ Airport,data=cities.df, main="Airport Distance vs Roomrent", ylab="Airport Distance", xlab="Roomrent", horizontal=TRUE)

freewifiandbfholiday <- xtabs(~FreeWifi + FreeBreakfast + HasSwimmingPool+IsWeekend+IsNewYearEve,data=cities.df)
ftable(freewifiandbfholiday)

library(vcd)
mosaic(freewifiandbfholiday, shade=TRUE, legend=TRUE, main="Relational factors")

library(car)
scatterplotMatrix(~RoomRent+ Airport+StarRating +HotelCapacity,data=cities.df)

scatterplotMatrix(~RoomRent+ FreeWifi+FreeBreakfast +HasSwimmingPool,data=cities.df)

#corrgram
library(corrgram)
corrgram(cities.df,order = TRUE, lower.panel = panel.shade,upper.panel = panel.pie,text.panel=panel.txt)
cor(cities.df[,c(4,5,6,7,8,11,12,13,17,18,19,20)])


#t tests
t.test(RoomRent,StarRating)
t.test(RoomRent,Airport)
t.test(RoomRent,FreeWifi&FreeBreakfast)
t.test(RoomRent,IsWeekend|IsNewYearEve)
t.test(RoomRent,HotelCapacity)

#Regression analysis
model1<- RoomRent~ IsMetroCity + CityRank+ IsTouristDestination+IsWeekend+IsNewYearEve+ Date+StarRating+FreeWifi+FreeBreakfast+HotelCapacity+HasSwimmingPool+Airport
fit1 <- lm(model1,data =cities.df)
summary(fit1)

library(leaps)
leap1 <- regsubsets(model1,data =cities.df,nbest=1)
plot(leap1,scale ="adjr2")

#refined model
model2<- RoomRent~ IsMetroCity+CityRank + IsTouristDestination+StarRating+HotelCapacity+HasSwimmingPool
fit2 <- lm(model2,data =cities.df)
summary(fit2)

library(coefplot)
coefplot(fit2,intercept = FALSE,outerCI =1.96,coefficients= c("IsMetroCity","Istourist","StarRating","HotelCapacity","Haspool","Airport"))

summary(fit1)$adj.r.squared
summary(fit2)$adj.r.squared
AIC(fit1)
AIC(fit2)


























