ooty.df[7:9] <- list(NULL)
ooty.df[9:10]<- list(NULL)
ooty.df[6]<- NULL

library(corrgram)
corrgram(ooty.df,order = TRUE, lower.panel = panel.shade,upper.panel = panel.pie,text.panel=panel.txt)
cor(ooty.df[,c(1,2,4,5,6,7)])

