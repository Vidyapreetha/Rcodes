table(kanpur.df$HotelName, kanpur.df$FreeWifi&kanpur.df$FreeBreakfast)
table(kanpur.df$HotelName , kanpur.df$IsNewYearEve|kanpur.df$IsWeekend)
par(mfrow=c(3,1))
with(kanpur.df,plot(kanpur.df$Airport , kanpur.df$RoomRent))
with(kanpur.df,plot(kanpur.df$FreeWifi&kanpur.df$FreeBreakfast , kanpur.df$RoomRent))
with(kanpur.df,plot(kanpur.df$IsNewYearEve|kanpur.df$IsWeekend,kanpur.df$RoomRent))
kanpur.df[1:5]<- list(NULL)
kanpur.df[3]<- NULL
kanpur.df[7:9] <- list(NULL)
kanpur.df[9:10]<- list(NULL)
kanpur.df[5]<- NULL
kanpur.df[6:7] <- list(NULL)

library(corrgram)
corrgram(kanpur.df,order = TRUE, lower.panel = panel.shade,upper.panel = panel.pie,text.panel=panel.txt)
cor(kanpur.df[,c(1,2,4,5)])




lfit<- lm(RoomRent~ Date + HotelName+ IsWeekend +HotelPincode + IsNewYearEve+ StarRating + Airport + FreeWifi+FreeBreakfast+HotelDescription +HotelCapacity+HasSwimmingPool,data=kanpur.df)
summary(lfit)

git<- lm(RoomRent~ HotelDescription, data = kanpur.df)
summary(git)
lfit<- lm(RoomRent~ IsWeekend + IsNewYearEve,data=kanpur.df)
summary(lfit)
# amritsar
amritsar.df <- read.csv(paste("13 Amritsar.csv", sep="")) #to read the file
View(amritsar.df)
summary(amritsar.df)
barchart(HotelName~RoomRent,data = amritsar.df)
barchart(HotelName~StarRating,data = amritsar.df)
library(lattice)
bwplot(HotelName ~Airport,data=amritsar.df,horizontal = TRUE)

table(amritsar.df$HotelName, amritsar.df$FreeWifi&amritsar.df$FreeBreakfast)
table(amritsar.df$HotelName , amritsar.df$IsNewYearEve|amritsar.df$IsWeekend)
par(mfrow=c(3,1))
with(amritsar.df,plot(amritsar.df$Airport , amritsar.df$RoomRent))
with(amritsar.df,plot(amritsar.df$FreeWifi&amritsar.df$FreeBreakfast , amritsar.df$RoomRent))
with(amritsar.df,plot(amritsar.df$IsNewYearEve|amritsar.df$IsWeekend,amritsar.df$RoomRent))
amritsar.df[1:5]<- list(NULL)
amritsar.df[3]<- NULL
amritsar.df[7:9] <- list(NULL)
amritsar.df[9:10]<- list(NULL)
library(corrgram)
corrgram(amritsar.df,order = TRUE, lower.panel = panel.shade,upper.panel = panel.pie,text.panel=panel.txt)
cor(amritsar.df[,c(1,2,4,5,6,7,8)])



lfit<- lm(RoomRent~ Date + HotelName+ IsWeekend + IsNewYearEve+ StarRating + Airport + FreeWifi+FreeBreakfast +HotelCapacity+HasSwimmingPool,data=amritsar.df)
summary(lfit)

has <- amritsar.df[ which(amritsar.df$FreeWifi=='1' & amritsar.df$FreeBreakfast=='1') , ] 
View(has)
median(has$RoomRent)

doesnthave<- amritsar.df[ which(amritsar.df$FreeWifi=='0' & amritsar.df$FreeBreakfast=='0') , ] 
median(doesnthave$RoomRent)

lfit<- lm(RoomRent~ IsWeekend + IsNewYearEve,data=amritsar.df)
summary(lfit)
