#goa
goa.df <- read.csv(paste("18 Goa.csv", sep="")) #to read the file
View(goa.df)
summary(goa.df)
dim(goa.df)
barchart(HotelName~ StarRating,data=goa.df)
library(lattice)
bwplot(HotelName ~Airport,data=goa.df,horizontal = TRUE)

table(goa.df$HotelName, goa.df$FreeWifi&goa.df$FreeBreakfast)
table(goa.df$HotelName , goa.df$IsNewYearEve|goa.df$IsWeekend)

par(mfrow=c(3,1))
with(goa.df,plot(goa.df$Airport , goa.df$RoomRent))
with(goa.df,plot(goa.df$FreeWifi&goa.df$FreeBreakfast , goa.df$RoomRent))
with(goa.df,plot(goa.df$IsNewYearEve|goa.df$IsWeekend,goa.df$RoomRent))

goa.df[1:5]<- list(NULL)
goa.df[3]<- NULL
goa.df[7:9] <- list(NULL)
goa.df[9:10]<- list(NULL)
library(corrgram)
corrgram(goa.df,order = TRUE, lower.panel = panel.shade,upper.panel = panel.pie,text.panel=panel.txt)

cor(goa.df$RoomRent,y=goa.df$IsNewYearEve,use="everything",method=c("spearman"))
cor(goa.df[,c(1,2,4,5,6,7,8)])
